{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "style": "plain",
          "template": " \uf0e7 \ue0b1",
          "type": "root"
        },
        {
          "background": "#546E7A",
          "foreground": "#26C6DA",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\uf7db",
            "mixed_threshold": 8,
            "style": "mixed"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "style": "plain",
          "template": "<#26C6DA>\ue0b1</> {{ .Status }}",
          "type": "poshgit"
        },
        {
          "background": "transparent",
          "foreground": "#546E7A",
          "style": "plain",
          "template": "\ue0b0",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} <#26C6DA>\ue0b3</>",
          "type": "session"
        },
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "properties": {
            "style": "austin",
            "threshold": 500
          },
          "style": "plain",
          "template": " {{ .FormattedMs }} \ufbab <#26C6DA>\ue0b3</>",
          "type": "executiontime"
        },
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#546E7A",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} \u276f",
          "type": "os"
        },
        {
          "foreground": "#546E7A",
          "style": "powerline",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} \u276f",
          "type": "dotnet"
        },
        {
          "foreground": "#546E7A",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \u276f",
          "type": "node"
        },
        {
          "foreground": "#546E7A",
          "style": "powerline",
          "template": " \ufcb5 {{ .Name }} ",
          "type": "shell"
        },
        {
          "foreground": "#D4E157",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
