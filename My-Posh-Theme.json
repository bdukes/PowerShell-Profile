{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "style": "plain",
          "type": "root"
        },
        {
          "background": "#546E7A",
          "foreground": "#26C6DA",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\uf7db",
            "mixed_threshold": 8,
            "style": "mixed"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "style": "plain",
          "type": "poshgit"
        },
        {
          "background": "transparent",
          "foreground": "#546E7A",
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "type": "session"
        },
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "properties": {
            "style": "austin",
            "threshold": 500
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "background": "#546E7A",
          "foreground": "#D4E157",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#546E7A",
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "#546E7A",
          "style": "powerline",
          "type": "dotnet"
        },
        {
          "foreground": "#546E7A",
          "style": "powerline",
          "type": "azfunc"
        },
        {
          "foreground": "#546E7A",
          "properties": {
            "fetch_package_manager": true
          },
          "style": "powerline",
          "type": "node"
        },
        {
          "foreground": "#546E7A",
          "style": "powerline",
          "type": "shell"
        },
        {
          "foreground": "#D4E157",
          "foreground_templates": ["{{ if .Error }}#FF5252{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
